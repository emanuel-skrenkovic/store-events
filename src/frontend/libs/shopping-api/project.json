{
  "root": "libs/shopping-api",
  "sourceRoot": "libs/shopping-api/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shopping-api/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shopping-api"],
      "options": {
        "jestConfig": "libs/shopping-api/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
